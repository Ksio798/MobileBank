@page "/Loginpage"
@inject NavigationManager NavigationManager
@using BankApp.Data
@inject Database db

<div class="page-background-class">
    <p id="wbs"><font color="#9b59b6" face="Helvetica">WorldSkills Bank</font></p>

    <svg width="54.000031" height="54.000000" viewBox="0 0 54 54" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">

        <defs>
            <pattern id="pattern_1_90" patternContentUnits="objectBoundingBox" width="1.000000" height="1.000000">
                <use xlink:href="#image1_9_0" transform="matrix(0.01,0,0,0.01,0,0)" />
            </pattern>
            <image id="image1_9_0" width="100.000000" height="100.000000" xlink:href="data:image/png;base64," />
        </defs>
        <rect id="icons8-museum-100" width="54.000000" height="54.000000" fill="url(#pattern_1_90)" />
    </svg>

    <button type="button" class="inp" @onclick="ShowLoginDialog">ВОЙТИ</button>


    <button type="button" class="ob" @onclick="NavigateToAddressPage">

        <svg width="30.000000" height="30.000000" viewBox="0 0 30 30" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">

            <defs>
                <pattern id="pattern_1_140" patternContentUnits="objectBoundingBox" width="1.000000" height="1.000000">
                    <use xlink:href="#image1_14_0" transform="matrix(0.01,0,0,0.01,0,0)" />
                </pattern>
                <image id="image1_14_0" width="100.000000" height="100.000000" xlink:href="data:image/png;base64," />
            </defs>
            <rect id="icons8-map-pin-100" width="30.000000" height="30.000000" fill="url(#pattern_1_140)" />
        </svg>

        Отделения и банкоматы
    </button>


    <button type="button" class="currency_rate" @onclick="NavigateToCoursePage">
        <svg width="30.000000" height="30.000000" viewBox="0 0 30 30" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">

            <defs>
                <pattern id="pattern_1_680" patternContentUnits="objectBoundingBox" width="1.000000" height="1.000000">
                    <use xlink:href="#image1_68_0" transform="matrix(0.007813,0,0,0.007813,0,0)" />
                </pattern>
                <image id="image1_68_0" width="128.000000" height="128.000000" xlink:href="data:image/png;base64," />
            </defs>
            <rect id="icons8-exchange-dollar-128" width="30.000000" height="30.000000" fill="url(#pattern_1_680)" />
        </svg>
        Курсы валют
        <span style="float:right">
            <font color="#988F8F" size="4">@DateTime.Today.ToString("dd.MM.yy")</font>

        </span>
        <br />
        <h class="marg">USD 90.08</h>
        <h class="marg"> EUR 98.57</h>
    </button>
</div>
@if (showDialog)
{
    <div class="dialog-backdrop">
        <div class="dialog-window">
            <h4>Вход</h4>
            <div class="form-group">
                <label for="login">Логин:</label>
                <input id="login" type="text" class="form-control" @bind="@login">
            </div>
            <div class="form-group">
                <label for="password">Пароль:</label>
                <input id="password" type="password" class="form-control" @bind="@password">
            </div>
            <p class="message">@message</p>
            <button @onclick="Login">Войти</button>
            <button @onclick="Cancel">Отмена</button>
        </div>
    </div>
}

@if (loggedIn)
{
    <p>Вы вошли как: @login</p>
}
@code {

    private void NavigateToAddressPage()
    {

        NavigationManager.NavigateTo("/address");
    }
    private void NavigateToCoursePage()
    {

        NavigationManager.NavigateTo("/course");
    }
}

